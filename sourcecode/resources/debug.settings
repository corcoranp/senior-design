[system]						; system configuration
version		= 1             	; system version
name 		= BLAZE				; ROBOT NAME

[logging]
debugLevel	= DEBUG				; Current Debug Level: "ERROR", "WARNING", "INFO", "DEBUG", "DEBUG1", "DEBUG2", "DEBUG3", "DEBUG4"
logFile		= blaze_debug.log	; name of the log file to save data to
appendLog	= true				; specifies whether to append to the log (true) or overwrite (false)

[user]
name 		= Peter Corcoran  	; Spaces around '=' are stripped
email 		= pmcorcor@uab.edu	; And comments (like this) ignored